<br><br><br><br><br><br>
<h1>Lista de tipo de novedad</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @tiponovedads.each do |tiponovedad| %>
      <tr>
        <td><%= tiponovedad.nombre %></td>
        <td><%= link_to 'Show', tiponovedad %></td>
        <td><%= link_to 'Edit', edit_tiponovedad_path(tiponovedad) %></td>
        <td><%= link_to 'Destroy', tiponovedad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tiponovedad', new_tiponovedad_path %>
